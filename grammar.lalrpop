use ::std::str::FromStr;


grammar;

pub body: Vec<Block> = {
    <es:(<block> ";")*> => es,
};

block: Block = {
    operation kind identifier "{"
};

identifier: Iden = {
    <n:r"[a-zA-Z_][a-zA-Z0-9_]*"> => Iden::new(n.to_owned()),
};

operation: Operation = {
    "add" => Keyword::Add,
    "update" => Keyword::Update,
}

kind: Kind = {
    "object" => Keyword::Object,
}
